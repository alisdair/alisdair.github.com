layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Alisdair McDiarmid</title>
 <link href="http://alisdair.mcdiarmid.org/feed.atom" rel="self"/>
 <link href="http://alisdair.mcdiarmid.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://alisdair.mcdiarmid.org/</id>
 <author>
   <name>Alisdair McDiarmid</name>
   <email>alisdair@mcdiarmid.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://alisdair.mcdiarmid.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://alisdair.mcdiarmid.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
